# Enhanced Development Playground Environment Configuration
# ========================================================

# Required: OpenAI API Key for AI agents
OPENAI_API_KEY=your_openai_api_key_here

# Optional: MongoDB Configuration (for generated backends)
MONGODB_URL=mongodb://localhost:27017
DATABASE_NAME=agno_development

# Optional: JWT Configuration (for authentication in generated apps)
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production

# Optional: Environment Setting
ENVIRONMENT=development

# Optional: Playground Configuration
PLAYGROUND_HOST=0.0.0.0
PLAYGROUND_PORT=7777

# Optional: Debug Settings
DEBUG=false
AGNO_LOG_LEVEL=INFO

# Optional: Edge Deployment Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:7777

# Optional: Performance Settings
MAX_WORKERS=4
TIMEOUT_SECONDS=300